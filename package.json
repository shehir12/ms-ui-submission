{
  "name": "ms-ui-submission",
  "version": "2.3.0",
  "engines": {
    "node": "12.13.0"
  },
  "description": "Customer-facing web application to manage submissions for contributions-based Employment and Support Allowance",
  "main": "./app/app.js",
  "files": [
    "app/**/*",
    "gulpfile.js"
  ],
  "scripts": {
    "start": "node ./app/app.js",
    "start:docker": "docker-compose up -d --build localstack localstack-init redis-cluster stub app",
    "stop:docker": "docker-compose down",
    "test": "npm run test:unit",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:smoke": "docker-compose up -d --build localstack localstack-init redis-cluster stub selenium app && docker-compose up --exit-code-from smoke_test smoke_test && docker-compose down",
    "test:journey": "docker-compose up -d --build localstack localstack-init redis-cluster stub selenium app && docker-compose up --exit-code-from journey_test journey_test && docker-compose down",
    "test:axe": "docker-compose up -d --build localstack localstack-init redis-cluster stub selenium app && docker-compose up --exit-code-from axe_test axe_test && docker-compose down",
    "quality:lint": "eslint -c node_modules/@dwp/eslint-config-base/.eslintrc.js .",
    "quality:lintfix": "eslint -c node_modules/@dwp/eslint-config-base/.eslintrc.js --fix .",
    "quality:coverage": "nyc --reporter=lcov npm test",
    "fill": "export PATH=$PATH:$(pwd)/node_modules/chromedriver/bin && node tools/complete-form.js",
    "package:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "errorpages": "./node_modules/.bin/gulp",
    "build": "./node_modules/.bin/gulp minifyjs"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.nonprod.dwpcloud.uk:health/ns-esa/ms-ui-submission.git"
  },
  "keywords": [],
  "author": "DWP Digital Health PDU",
  "license": "ISC",
  "dependencies": {
    "@dwp/govuk-casa": "4.0.8",
    "body-parser": "1.19.0",
    "connect-redis": "5.0.0",
    "cookie-parser": "^1.4.5",
    "csurf": "1.11.0",
    "del": "6.0.0",
    "dwp-cryptoservice": "2.2.2",
    "dwp-winston-format": "1.2.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "govuk-frontend": "3.9.1",
    "gulp": "4.0.2",
    "gulp-base64-v2": "1.0.4",
    "gulp-inline": "0.1.3",
    "gulp-inline-assets": "0.1.1",
    "gulp-minify": "3.1.0",
    "gulp-nunjucks-render": "2.2.3",
    "gulp-replace": "1.0.0",
    "hashids": "2.2.1",
    "ioredis": "4.17.3",
    "lodash.isequal": "4.5.0",
    "lodash.merge": "4.6.2",
    "moment": "2.29.1",
    "notifications-node-client": "4.9.0",
    "nunjucks": "3.2.2",
    "request-promise-native": "1.0.9",
    "session-file-store": "1.5.0",
    "uuid": "8.3.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@dwp/eslint-config-base": "4.1.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-subset": "1.6.0",
    "chromedriver": "86.0.0",
    "eslint": "7.11.0",
    "faker": "5.1.0",
    "mocha": "^8.1.3",
    "nyc": "15.1.0",
    "rewire": "5.0.0",
    "selenium-webdriver": "4.0.0-alpha.7",
    "sinon": "9.2.0",
    "sinon-chai": "3.5.0"
  }
}
